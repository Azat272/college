<Window x:Class="CollegeApp.AddDisciplineWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Добавить дисциплину" Height="200" Width="300">
    <StackPanel Margin="10">
        <TextBlock Text="Название дисциплины"/>
        <TextBox x:Name="disciplineNameTb"/>

        <Button Content="Добавить" Click="AddBtn_Click" Margin="10"/>
    </StackPanel>
</Window>
